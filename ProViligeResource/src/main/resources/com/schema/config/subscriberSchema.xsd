<xs:schema attributeFormDefault="unqualified"
	elementFormDefault="qualified"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.provigil.com/subscriber">
	<xs:element name="subscriptions">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="subscription" maxOccurs="unbounded"
					minOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element type="xs:byte" name="id" />
							<xs:element type="xs:short" name="area" />
							<xs:element type="xs:string" name="plan" />
							<xs:element type="xs:string" name="location" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>